	{{ form_row(form.protocole.echographieVasculaire.axesJambiersGauches, {'attr': {'class': 'input'} }) }}
</div>

<div class="form-space">
	<div class="column">
		{{ form_row(form.protocole.echographieVasculaire.IPSReposDroit, {'attr': {'class': 'input'} }) }}
		{{ form_row(form.protocole.echographieVasculaire.IPSReposGauche, {'attr': {'class': 'input'} }) }}
	</div>

	<div class="column">
		{{ form_row(form.protocole.echographieVasculaire.IPSGrosOrteilDroit, {'attr': {'class': 'input'} }) }}
		{{ form_row(form.protocole.echographieVasculaire.IPSGrosOrteilGauche, {'attr': {'class': 'input'} }) }}
	</div>

	<div class="form-space">
		{{ form_row(form.protocole.echographieVasculaire.testStrandnessDistanceGene, {'attr': {'class': 'input'}, 'useStyle': true }) }}
		{{ form_row(form.protocole.echographieVasculaire.testStrandnessDistanceMax, {'attr': {'class': 'input'}, 'useStyle': true }) }}
		<div class="def">* compris entre 0 et 1000, 9999 si non réalisé</div>
	</div>
	<div class="form-space">
		{{ form_row(form.protocole.echographieVasculaire.arretPour, {'attr': {'class': 'input'}, 'useStyle': true }) }}
	</div>

	<div class="column">
		{{ form_row(form.protocole.echographieVasculaire.IPSEffortDroit, {'attr': {'class': 'input'} }) }}
		{{ form_row(form.protocole.echographieVasculaire.IPSEffortGauche, {'attr': {'class': 'input'} }) }}
	</div>

	<div class="form-space column">
		{{ form_row(form.protocole.echographieVasculaire.vitesseOndePouls, {'attr': {'class': 'input'}, 'useStyle': true }) }}
	</div>
</div>

{{ form_row(form.protocole.echographieVasculaire.save, {'attr': {'class': 'form-btn btn-save', 'disabled': freezeDatabase} }) }}
