<h1 id="echographie-vasculaire">Échographie vasculaire</h1>
<h3 id="tsa">Schéma TSA</h3>
<div class="form-space schema-tsa">
	<img src={{ asset('assets/images/tsa.png') }} alt="Schéma TSA"/>
	{{ form_row(form.protocole.echographieVasculaire.carotideInterneDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.carotideInterneGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.EIMDroit, {'attr': {'class': 'input special'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.EIMGauche, {'attr': {'class': 'input special'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.vertebraleDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.vertebraleGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.carotideCommuneDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.carotideCommuneGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.sousClaviereDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.sousClaviereGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.TSAAorte, {'attr': {'class': 'input'}, 'useStyle': true }) }}

	{{ form_row(form.protocole.echographieVasculaire.volumeCarotideDroite, {'attr': {'class': 'input special'}, 'useStyle': true }) }}
	{{ form_row(form.protocole.echographieVasculaire.volumeCarotideGauche, {'attr': {'class': 'input special'}, 'useStyle': true }) }}
	{{ form_row(form.protocole.echographieVasculaire.chargeAtheromeTotale, {'attr': {'class': 'input special'}, 'useStyle': true }) }}
</div>

<h3 id="membres-inferieur">Schéma membres inférieur</h3>
<div class="form-space schema-membres-inferieur">
	<img src={{ asset('assets/images/membres_inferieur.jpg') }} alt="Schéma membres inférieur"/>
	{{ form_row(form.protocole.echographieVasculaire.membresInferieurAorte, {'attr': {'class': 'input'}, 'useStyle': true }) }}
	{{ form_row(form.protocole.echographieVasculaire.volumeFemoralDroit, {'attr': {'class': 'input special'}, 'useStyle': true }) }}
	{{ form_row(form.protocole.echographieVasculaire.volumeFemoralGauche, {'attr': {'class': 'input special'}, 'useStyle': true }) }}
	{{ form_row(form.protocole.echographieVasculaire.iliaqueDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.iliaqueGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.femoraleCommuneDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.femoraleCommuneGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.femoraleSuperficielleDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.femoraleSuperficielleGauche, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.popliteDroite, {'attr': {'class': 'input'} }) }}
	{{ form_row(form.protocole.echographieVasculaire.popliteGauche, {'attr': {'class': 'input'} }) }}
